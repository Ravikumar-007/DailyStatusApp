<script>
var pageSize=9;
var href=0;
    function solrCheck() {	
        if ($("#solrTextBox").val() != "")
           
            return true
        else
            return false
            
    }
    $(document).ready(function()
    		{   

    	$("#getPagesize").change(function()
    		    {
    		        var id=$(this).val();
    		        var dataString = 'id='+ id;
    		        pageSize=id;
    		        showMore();
    		         return false;
    		    });
	    
    		   			 $('.pagination-custom a').click(function(){
    		        
    		           $('.pagination-custom a').removeClass("active") 
    		            $(this).toggleClass("active");
    		            href = $(".active").attr('href');
    		            href=href-1;
    		           	showMore();
    		           	return false;
    		           		    });
      		    
    		});



    function showMore() {
    	var newdata="";
	    var datadatailed="";
        var searchText=$('.search-form input').val();
        var paramString = "&pageSize=" + pageSize + "&startPage=" + href+"&solrTextValue="+searchText ;
        $.ajax({
            url: "/solrSearch/searchSolr",
            type: "get",
            data: paramString,
            success: function(data) {
              console.log("data  "+data.jsonArray.length);
               var total=data.jsonArray.length;
               for(var i=0;i<data.jsonArray.length-1;i++)
               {   
   				  var date=new Date(data.jsonArray[i].DATE);
				  var day=date.toString().substring(8,10);
				  var month=date.toString().substring(4,7);
				  var year=date.toString().substring(13,15); 
                  datadatailed="<li><div class='journal-date'>"+day+"<span>"+month+"-"+year+"</span></div><a href='/blogs/"+data.jsonArray[i].TITLE+"' class='journal-link icon-coffee'>Read More</a><div class='journal-content'><h3>"+data.jsonArray[i].TITLE+"</h3><span>"+data.jsonArray[i].BODY+"</span><br>Posted By:"+data.jsonArray[i].AUTHOR+"</div></li>";
                  newdata=newdata+""+datadatailed;
               }
           	 $("#searchedBlogs").html("<ul class='list-journal'>"+newdata+"</ur>");
                
            }
         
        });

    }
	
</script>
