        <script>
            $(window).load(function() {
                $('body').addClass('blue');
            });
            var startcall = 1;
            var tag = "20188";
            var category = "Archives";
            var totalBlogs = "43";
            function showMore() {
                var paramString = "&start=" + startcall + "&tag=" + tag + "&category=" + category;
                $.ajax({
                    url: "/blog/tagBloglist",
                    type: "post",
                    data: paramString,
                    success: function(data) {
                        startcall += 1;
                        $('ul#more-articles').append(data);
                        if ((startcall * 5) >= totalBlogs)
                            $("#moreButton").hide();
                    }
                });
            }
        </script>
